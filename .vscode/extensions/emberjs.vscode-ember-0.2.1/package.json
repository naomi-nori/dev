{
	"name": "vscode-ember",
	"displayName": "Ember Language Server",
	"description": "Provides features like auto complete, goto definition and diagnostics for Ember.js projects",
	"author": "Thomas Sauer <t-sauer@outlook.de>",
	"license": "MIT",
	"version": "0.2.1",
	"publisher": "emberjs",
	"icon": "icon.png",
	"keywords": [
		"Ember.js",
		"Ember-CLI",
		"Glimmer",
		"Handlebars",
		"Language server"
	],
	"engines": {
		"vscode": "^1.22.2"
	},
	"preview": true,
	"categories": [
		"Programming Languages",
		"Linters"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/emberwatch/vscode-ember"
	},
	"bugs": {
		"url": "https://github.com/emberwatch/vscode-ember/issues"
	},
	"activationEvents": [
		"onLanguage:handlebars",
		"onLanguage:javascript",
		"workspaceContains:ember-cli-build.js"
	],
	"main": "./out/src/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"update-vscode": "node ./node_modules/vscode/bin/install",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.33",
		"@types/node": "^6.0.52",
		"typescript": "^2.1.5",
		"vscode": "^1.1.14"
	},
	"dependencies": {
		"@emberwatch/ember-language-server": "0.2.1",
		"vscode-languageclient": "^4.1.3"
	},
	"__metadata": {
		"id": "a6d27ca9-1c67-4132-a7c1-fff75f47c459",
		"publisherId": "83b77bd0-61f3-447e-bd38-99d39d030ffe",
		"publisherDisplayName": "emberjs"
	}
}